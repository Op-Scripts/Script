-- Scripts:

local function ZESEI_fake_script() -- Anims.LocalScript 
	local script = Instance.new('LocalScript', Anims)

	script.Parent.MouseButton1Down:connect(function()
		script.Parent.Parent.AnimTab.Visible = true
		script.Parent.Parent.BlatentTab.Visible = false
	end)
end
coroutine.wrap(ZESEI_fake_script)()

local function PBLXAD_fake_script() -- Blatent.LocalScript 
	local script = Instance.new('LocalScript', Blatent)

	script.Parent.MouseButton1Down:connect(function()
		script.Parent.Parent.AnimTab.Visible = false
		script.Parent.Parent.BlatentTab.Visible = true
	end)
end
coroutine.wrap(PBLXAD_fake_script)()
